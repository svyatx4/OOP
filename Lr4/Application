public class Application extends Software {
    protected type type;
    
    public Application(String N, String V, String M, type T) {
        super(N, V, M);
        type = T;
    }

    @Override
    public String getName() {
        return "Application Software >" +
        "\n Name: " + this.name +
        "\n Version: " + this.version +
        "\n Memory Space: " + this.memory +
        "\n Type: " + type;
    }
}
