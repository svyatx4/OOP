public final class FreeInstrument extends Instrumental {
    private int date;

    public FreeInstrument(String N, String V, String M, boolean ide, int D) {
        super(N, V, M, ide);
        date = D;
    }

    @Override
    public String getName() {
        return "Free Instrumental Software >" +
        "\n Name: " + name +
        "\n Version: " + version +
        "\n Memory Space: " + memory +
        "\n Integrated Development Environment: " + IDE +
        "\n Release Date: " + date;
    }
}
